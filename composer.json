{
    "name": "pavinthan/laravel-dynamodb",
    "description": "A DynamoDB based Eloquent model and Query builder for Laravel.",
    "keywords": [
        "laravel",
        "dynamodb"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pavinthan/laravel-dynamodb/issues",
        "source": "https://github.com/pavinthan/laravel-dynamodb"
    },
    "authors": [
        {
            "name": "Pavinthan Sivakumar",
            "homepage": "https://pavinthan.dev"
        }
    ],
    "require": {
        "illuminate/support": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/events": "^8.0",
        "aws/aws-sdk-php": "^3.180"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.0",
        "mockery/mockery": "^1.3.1",
        "doctrine/dbal": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "BrainLK\\DynamoDb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrainLK\\DynamoDb\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BrainLK\\DynamoDb\\DynamoDbServiceProvider",
                "BrainLK\\DynamoDb\\DynamoDbQueueServiceProvider"
            ]
        }
    }
}